{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.borealis.cool" }],
      "destination": "https://borealis.cool/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/displayBoard.pdf" }
  ],
  "headers": [
    {
      "source": "/displayBoard.pdf",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    }
  ]
}